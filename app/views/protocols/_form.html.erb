<%= form_for(@protocol) do |f| %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>


  <div class="field">
    <%= f.label :procedure %><br>
    <%= f.text_area :procedure %>
  </div>

  <div class="field">
    <%= f.label :document %><br>
    <%= f.file_field :document %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<br />
<%= link_to "Return back to list of protocols", protocols_path %>
