<div class="callout new-page">
  <div class="small-6 small-centered columns">
    <h1>Create Order Request</h1>
    <%= link_to "Back to orders", orders_path %>
    <br />
    <%= form_for(@order) do |f| %>
       <%= f.label :vendor_id, "Vendor:" %>
       <%= f.select :vendor_id, options_from_collection_for_select(Vendor.all, :id, :name) %>
      <div class="action">
        <%= f.submit %>
      </div>
    <% end %>
  </div>
</div>
